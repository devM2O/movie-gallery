    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <!-- Page level plugins -->
    <script src="vendorAdmin/datatables/jquery.dataTables.min.js"></script>
    <script src="vendorAdmin/datatables/dataTables.bootstrap4.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="jsAdmin/demo/datatables-demo.js"></script>



</body>

</html>
