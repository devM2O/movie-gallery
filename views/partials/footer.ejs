<script type="text/javascript">
  $(".editItem").click(function() {
    var rD = $(this).attr('rD');
    var vN = $(this).attr('vN');
    var vR = $(this).attr('vR');
    var vRV = $(this).attr('vRV');
    var vL = $(this).attr('vL');
    var vImg = $(this).attr('vImg');
    var vID = $(this).attr('vID');
    var cat = $(this).attr('cat');
    var po = $(this).attr('po');
    var url = "/uploads/";

    var catV;
    if (cat == "Bollywood") {
      catV = "0"
    } else if (cat == "Animation") {
      catV = "1"
    } else if (cat == "Action") {
      catV = "2"
    } else if (cat == "Comedy") {
      catV = "3"
    } else if (cat == "Horror") {
      catV = "4"
    } else {
      catV = "5"
    }

     $("input[name=options][value=" + po + "]").prop('checked', true);
    document.getElementById("mySelect").selectedIndex = catV;
    document.getElementById("EditImg").setAttribute("src", url + vImg);
    document.getElementById("EditImg1").setAttribute("value", vImg);
    document.getElementById("vdID").setAttribute("value", vID);
    document.getElementById("rating").setAttribute("value", vR);
    document.getElementById("movieN").setAttribute("value", vN);
    document.getElementById("release").setAttribute("value", rD);
    document.getElementById("dLink").setAttribute("value", vL);
    document.getElementById("review").value = vRV;

  });

  $(".deleteItem").click(function() {
    var id = $(this).attr('id');
    var url = "/delete/";
    document.getElementById("deleteHref").setAttribute("href", url + id);
  });
  $(".removeItem").click(function() {
    var id = $(this).attr('id');
    var url = "/remove/";
    document.getElementById("removeHref").setAttribute("href", url + id);
  });


</script>

