<% if(typeof errors != 'undefined'){ %>

<% errors.forEach(function (error) { %>
<div class="alert alert-warning alert-dismissible fade show" role="alert">
  <%= error.msg %>
  &nbsp&nbsp<i class="fas fa-exclamation-triangle"></i>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<%});%>

<% } %>


<% if(success_msg != '') {%>

<div class="alert alert-success alert-dismissible fade show sweetSAl text-center" role="alert">
  <%= success_msg %>
  &nbsp&nbsp<i class="far fa-check-circle" style="color:green;"></i>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>

<% } %>


<% if(error_msg != '') {%>

<div class="alert alert-warning alert-dismissible fade show sweetEAl text-center" role="alert">
  <%= error_msg %>
  &nbsp&nbsp<i class="fas fa-exclamation-triangle"></i>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>

</div>


<% } %>

<% if(error != '') {%>

<div class="alert alert-warning alert-dismissible fade show" role="alert">
  <%= error %>
  &nbsp&nbsp<i class="fas fa-exclamation-triangle"></i>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>

<% } %>
